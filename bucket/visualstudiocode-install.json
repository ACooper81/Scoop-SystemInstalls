        }
    },
    "installer": {
        "script": "Invoke-ExternalCommand \"$dir\\setup.exe\" -ArgumentList @('/VERYSILENT', '/MERGETASKS=!runcode,!desktopicon,!quicklaunchicon,addcontextmenufiles,addcontextmenufolders,associatewithfiles,addtopath') -RunAs | Out-Null"
    },
    "uninstaller": {
        "script": [
            "$ProgramFilesX86Path = [Environment]::GetFolderPath(\"ProgramFilesX86\")",
            "& \"$ProgramFilesX86Path\\Microsoft VS Code\\unins000.exe\" /VERYSILENT /SUPPRESSMSGBOXES /NORESTART"
        ]
    },
    "checkver": {
        "url": "https://code.visualstudio.com/sha?build=stable",
        "jp": "$.products[?(@.platform.os == 'win32-x64-archive')].name"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://code.visualstudio.com/docs/?dv=win64#/setup.exe"
            },
            "32bit": {
                "url": "https://code.visualstudio.com/docs/?dv=win32#/setup.exe"
            }
        }
